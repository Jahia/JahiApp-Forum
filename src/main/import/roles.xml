<?xml version="1.0" encoding="UTF-8"?>
<roles jcr:primaryType="jnt:roles"
       xmlns:jcr="http://www.jcp.org/jcr/1.0"
       xmlns:j="http://www.jahia.org/jahia/1.0"
       xmlns:jnt="http://www.jahia.org/jahia/nt/1.0">
    <forum-contributor jcr:primaryType="jnt:role"
                       j:permissionNames="createPost createTopic"
                       j:roleGroup="live-role"/>

    <forum-contributor jcr:primaryType="jnt:role"
             j:permissionNames="createTopic createPost"/>

    <jahiapp-user jcr:primaryType="jnt:role"
                  j:permissionNames="createRoom createSection"/>

    <editor jcr:primaryType="jnt:role">
        <editor-in-chief jcr:primaryType="jnt:role"
                     j:permissionNames="forum"
                     j:privilegedAccess="true"
            />
    </editor>
</roles>
